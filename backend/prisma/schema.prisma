// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./projex.db"
}

model User {
  id       Int    @id() @default(autoincrement())
  name     String
  email    String
  password String
}

model Property {
  id             Int  @id() @default(autoincrement())
  sale_price     Int
  purchase_price Int?
}
